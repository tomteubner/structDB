 &GLOBAL
   PRINT_LEVEL  MEDIUM
   PROJECT_NAME CO2
   RUN_TYPE  GEO_OPT
   SAVE_MEM  T
 &END GLOBAL
 &MOTION
   &GEO_OPT
     OPTIMIZER  BFGS
     MAX_ITER  1500
     MAX_FORCE     5.0000000000000002E-05
     STEP_START_VAL  1
   &END GEO_OPT
 &END MOTION
 &FORCE_EVAL
   METHOD  QS
   &DFT
     UKS  T
     CHARGE  0
     &SCF
       MAX_SCF  400
       EPS_SCF      1.0000000000000000E-07
       SCF_GUESS  ATOMIC
       &OT  T
         MINIMIZER  DIIS
         PRECONDITIONER  FULL_ALL
       &END OT
     &END SCF
     &QS
       EXTRAPOLATION  PS
       EXTRAPOLATION_ORDER  4
       METHOD  GAPW
     &END QS
     &MGRID
       CUTOFF         3.0000000000000000E+02
       REL_CUTOFF     3.0000000000000000E+01
     &END MGRID
     &XC
       &XC_FUNCTIONAL  NO_SHORTCUT
         &PBE  T
         &END PBE
       &END XC_FUNCTIONAL
       &VDW_POTENTIAL
         POTENTIAL_TYPE  PAIR_POTENTIAL
         &PAIR_POTENTIAL
           R_CUTOFF     1.2000000000000000E+01
           TYPE  DFTD3
           PARAMETER_FILE_NAME dftd3.dat
           REFERENCE_FUNCTIONAL PBE
           CALCULATE_C9_TERM  F
         &END PAIR_POTENTIAL
       &END VDW_POTENTIAL
     &END XC
     &POISSON
       POISSON_SOLVER  MULTIPOLE
       PERIODIC  NONE
       &MULTIPOLE
         RCUT     8.0000000000000000E+00
       &END MULTIPOLE
     &END POISSON
   &END DFT
   &SUBSYS
     &CELL
       A     2.7000000000000000E+01    0.0000000000000000E+00    0.0000000000000000E+00
       B     0.0000000000000000E+00    2.7000000000000000E+01    0.0000000000000000E+00
       C     0.0000000000000000E+00    0.0000000000000000E+00    2.7000000000000000E+01
       PERIODIC  NONE
       MULTIPLE_UNIT_CELL  1 1 1
     &END CELL
     &COORD
C    -0.00105   -0.00105    0.41414
O     0.83741    0.83741    0.53665
O    -0.84373   -0.84373    0.53727
       UNIT angstrom
       SCALED  F
     &END COORD
     &KIND Si
       BASIS_SET DZVP-MOLOPT-GTH
       POTENTIAL GTH-PBE-q4
     &END KIND
     &KIND Ga
       BASIS_SET DZVP-MOLOPT-GTH
       POTENTIAL GTH-PBE-q13
     &END KIND
     &KIND C
       BASIS_SET DZVP-MOLOPT-GTH
       POTENTIAL GTH-PBE-q4
     &END KIND
     &KIND O
       BASIS_SET DZVP-MOLOPT-GTH
       POTENTIAL GTH-PBE-q6
     &END KIND
     &KIND H
       BASIS_SET TZVP-MOLOPT-GTH
       POTENTIAL GTH-PBE-q1
     &END KIND
     &KIND F
       BASIS_SET DZVP-MOLOPT-GTH
       POTENTIAL GTH-PBE-q7
     &END KIND
     &KIND N
       BASIS_SET DZVP-MOLOPT-GTH
       POTENTIAL GTH-PBE-q5
     &END KIND
     &KIND Cl
       BASIS_SET DZVP-MOLOPT-GTH
       POTENTIAL GTH-PBE-q7
     &END KIND
     &KIND Ti
       BASIS_SET DZVP-MOLOPT-SR-GTH
       POTENTIAL GTH-PBE-q12
     &END KIND
     &TOPOLOGY
       MULTIPLE_UNIT_CELL  1 1 1
       &CENTER_COORDINATES  T
       &END CENTER_COORDINATES
     &END TOPOLOGY
   &END SUBSYS
 &END FORCE_EVAL
