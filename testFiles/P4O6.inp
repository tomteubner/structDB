 &GLOBAL
   PRINT_LEVEL  MEDIUM
   PROJECT_NAME P4O6
   RUN_TYPE  GEO_OPT
   SAVE_MEM  T
 &END GLOBAL
 &MOTION
   &GEO_OPT
     OPTIMIZER  BFGS
     MAX_ITER  1500
     MAX_FORCE     5.0000000000000002E-05
     STEP_START_VAL  1
   &END GEO_OPT
 &END MOTION
 &FORCE_EVAL
   METHOD  QS
   &DFT
     UKS  T
     CHARGE  0
     &SCF
       MAX_SCF  200
       EPS_SCF      1.0000000000000000E-07
       SCF_GUESS  ATOMIC
       &OT  T
         MINIMIZER  DIIS
         PRECONDITIONER  FULL_ALL
       &END OT
     &END SCF
     &QS
       EXTRAPOLATION  PS
       EXTRAPOLATION_ORDER  4
       METHOD  GAPW
